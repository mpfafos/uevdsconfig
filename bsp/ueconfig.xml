						<ProtocolType>1</ProtocolType>
						<Identificator>123456</Identificator>
<!-- TBD , Example -->	<DestinationName>Ziel123456</DestinationName>				<!-- Zielname ergänzt 30.09.2020-->
						<PathType>2</PathType>
						<Target>
							<TargetPriority>1</TargetPriority>						<!-- Ziel-Prio -->
